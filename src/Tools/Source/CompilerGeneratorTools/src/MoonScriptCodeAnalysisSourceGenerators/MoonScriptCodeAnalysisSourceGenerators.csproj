<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Luna.Compilers.Generators</RootNamespace>
    <ImplicitUsings>true</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AutoGenerateBindingRedirects>True</AutoGenerateBindingRedirects>
    <RuntimeIdentifiers>$(LunaPortableRuntimeIdentifiers)</RuntimeIdentifiers>
    <IsShipping>false</IsShipping>
    <ExcludeFromSourceBuild>false</ExcludeFromSourceBuild>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\..\..\..\Compilers\Core\Portable\Symbols\LanguageNames.cs" Link="LanguageNames.cs" />
    <Compile Include="..\..\..\..\..\Compilers\MoonScript\Portable\Errors\ErrorCode.cs" Link="ErrorCode.cs" />
    <Compile Include="..\..\..\..\..\Compilers\MoonScript\Portable\Syntax\SyntaxKind.cs" Link="SyntaxKind.cs" />
    <Compile Include="..\..\..\..\..\Compilers\MoonScript\Portable\Syntax\SyntaxKindFacts.cs" Link="SyntaxKindFacts.cs" />
  </ItemGroup>

  <Import Project="..\Shared\CodeAnalysisGenerators.Shared.projitems" Label="Shared" />

  <ItemGroup>
    <InternalsVisibleTo Include="MoonScriptErrorFactsGenerator" />
    <InternalsVisibleTo Include="MoonScriptSyntaxGenerator" />
  </ItemGroup>

</Project>
