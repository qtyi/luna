<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <RootNamespace></RootNamespace>
    <EnableDefaultItems>false</EnableDefaultItems>
    <IsShipping>false</IsShipping>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\README.md" />
    <Compile Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\Analyzers\DoNothingAnalyzer.cs" />
    <Compile Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\Analyzers\DoNothingGenerator.cs" />
    <Compile Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\Analyzers\DoNothingSuppressor.cs" />
    <Compile Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\ResourceLoader.cs" />
    <Compile Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\TestKeys.cs" />
    <Compile Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\TestResources.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="$(DotNetRoslynRepositoryDirectory)src\Compilers\Test\Resources\Core\SymbolsTests\nativeCOFFResources.obj" />
  </ItemGroup>
  
  <ItemGroup>
    <InternalsVisibleFrom Include="Microsoft.CodeAnalysis" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="$(MicrosoftCodeAnalysisVersion)" />
  </ItemGroup>

</Project>