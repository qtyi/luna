    <Compile Include="..\..\..\Test\Shared\System.Collections.Generic\Tree`1.cs" Link="System.Collections.Generic\Tree`1.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\Test\tests\*.lua" />
  </ItemGroup>

  <ItemGroup>
    <!-- InternalsVisibleFrom -->
